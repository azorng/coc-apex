  "compilerOptions": {
    "composite": true,
    "outDir": "./out",
    "rootDir": ".",
    "declaration": true,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "module": "commonjs",
    "target": "es6",
    "lib": [  "es6", "dom" ]
  },
  "exclude": [
    "node_modules",
    "out"
  ]
}
